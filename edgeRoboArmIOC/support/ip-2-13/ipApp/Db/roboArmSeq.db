record(bo,    "$(P)$(A)start_moves") {
  field(DESC, "start seq moves")
  field(ZNAM, "STOP")
  field(ONAM, "RUN")
  field(FLNK, "$(P)$(A)send_cmd_seq.PROC  PP  MS")
}

record(transform, "$(P)$(A)all_stop") {
  field(DESC, "send the stop motion command")
  field(CLCA, "0")
  field(CLCB, "0")
  field(CLCC, "0")
  field(CLCD, "0")
  field(CLCE, "0")
  field(CLCF, "0")
  field(CLCG, "0")
  field(CLCH, "0")
  field(CLCI, "0")
  field(CLCJ, "0")
  field(OUTA, "$(P)$(A)grip_move.VAL           NPP NMS")
  field(OUTB, "$(P)$(A)wrist_move.VAL          NPP NMS")
  field(OUTC, "$(P)$(A)elbow_move.VAL          NPP NMS")
  field(OUTD, "$(P)$(A)shoulder_move.VAL       NPP NMS")
  field(OUTE, "$(P)$(A)base_move.VAL           NPP NMS")
  field(OUTF, "$(P)$(A)grip_move_seq.VAL       NPP NMS")
  field(OUTG, "$(P)$(A)wrist_move_seq.VAL      NPP NMS")
  field(OUTH, "$(P)$(A)elbow_move_seq.VAL      NPP NMS")
  field(OUTI, "$(P)$(A)shoulder_move_seq.VAL   NPP NMS")
  field(OUTJ, "$(P)$(A)base_move_seq.VAL       NPP NMS")
  field(FLNK, "$(P)$(A)send_cmd.PROC            PP  MS")
}

record(calc,  "$(P)$(A)send_cmd") {
  field(DESC, "send the motion command")
  field(INPA, "$(P)$(A)grip_move.RVAL      NPP NMS")
  field(INPB, "$(P)$(A)wrist_move.RVAL     NPP NMS")
  field(INPC, "$(P)$(A)elbow_move.RVAL     NPP NMS")
  field(INPD, "$(P)$(A)shoulder_move.RVAL  NPP NMS")
  field(CALC, "A+B+C+D")
  field(FLNK, "$(P)$(A)send_cmd_2.PROC      PP  MS") 
}

record(calc,  "$(P)$(A)send_cmd_seq") {
  field(DESC, "send the motion command")
  field(INPA, "$(P)$(A)grip_move_seq.RVAL      NPP NMS")
  field(INPB, "$(P)$(A)wrist_move_seq.RVAL     NPP NMS")
  field(INPC, "$(P)$(A)elbow_move_seq.RVAL     NPP NMS")
  field(INPD, "$(P)$(A)shoulder_move_seq.RVAL  NPP NMS")
  field(CALC, "A+B+C+D")
  field(FLNK, "$(P)$(A)send_cmd_seq_2.PROC      PP  MS") 
}

record(scalcout, "$(P)$(A)send_cmd_2") {
  field(DESC, "send the motion command")
  field(INPA, "$(P)$(A)send_cmd.VAL        NPP NMS")
  field(INPB, "$(P)$(A)base_move.RVAL      NPP NMS")
  field(INPC, "$(P)$(A)led_onoff.VAL       NPP NMS")
  field(CALC, "STR(A)+' '+STR(B)+' '+STR(C)")
  field(OUT,  "$(P)$(A)send_cmd_str.VAL     PP  MS") 
}

record(scalcout, "$(P)$(A)send_cmd_seq_2") {
